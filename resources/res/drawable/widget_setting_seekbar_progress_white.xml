<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/background">
        <shape android:tint="#80fafafa" android:shape="rectangle">
            <size android:height="@dimen/widget_setting_seek_bar_progress_size"/>
        </shape>
    </item>
    <item android:gravity="left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:tint="#fafafa" android:shape="rectangle">
                <size android:height="@dimen/widget_setting_seek_bar_progress_size"/>
            </shape>
        </scale>
    </item>
</layer-list>
